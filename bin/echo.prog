#!/usr/bin/env python3
import lib.libdiscord.TP
#for lib.libdiscord
import sys

def main(FIT:str, SR:str) -> int:
    
    t = lib.libdiscord.TP.DIS_TP(FIT)
    if ((SR == "") or (SR == None)):
        t.TEXT("NO TEXT ARG")
    t.TEXT(SR)
    #print(t.UID())
    return 0


sys.exit(main(sys.argv[1], sys.argv[2]))
