#!/usr/bin/env python3
from libdiscord.TP import DIS_TP
from libdiscord.color import hex_color
import random
import sys
import os
import io

DIR = os.path.abspath(".")

def main(FIT:str, GN:str) -> int:
    c = hex_color(0x84, 0xF8, 0xF4)
    d = os.listdir(DIR)
    s:str = ""
    for v in d:
        if (not v.startswith('.')):
            s = s + v + '\n'
    t = DIS_TP(FIT) 
    t.ENBED("ls '/':\n", s, CL=c.__int__())
    return 0

sys.exit(main(sys.argv[1], ""))