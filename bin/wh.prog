#!/usr/bin/env python3
from lib.libdiscord.TP import DIS_TP
from lib.libdiscord.color import hex_color
import sys

def main(FIT:str) -> int:
    c = hex_color(0x4F, 0xF9, 0xEA)
    t = DIS_TP(FIT) 
    MSG = f"Hay {t.USER_MENTI()}"
    t.ENBED(f"Welcome {t.USER()} to {t.GNAME()}", MSG, FOOTER=f"Hi {t.USER()}", CL=c.FULL)
    return 0

sys.exit(main(sys.argv[1]))
